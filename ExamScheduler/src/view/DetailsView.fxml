<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="431.0" prefWidth="370.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.DetailsViewController">
    <children>
        <VBox alignment="CENTER" layoutX="47.0" layoutY="108.0" prefHeight="266.0" prefWidth="276.0">
            <children>
                <Button mnemonicParsing="false" onAction="#planAnExamButtonPressed" prefHeight="62.0" prefWidth="184.0"
                        text="Plan an exam">
                    <VBox.margin>
                        <Insets bottom="30.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#manageDataButtonPressed" prefHeight="58.0" prefWidth="184.0"
                        text="Manage data">
                    <VBox.margin>
                        <Insets bottom="30.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#checkScheduleButtonPressed" prefHeight="55.0"
                        prefWidth="186.0" text="Check Schedule">
                    <VBox.margin>
                        <Insets bottom="30.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
        <Text layoutX="39.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Select what you would like to do:">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
